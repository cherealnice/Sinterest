<div class='page'>
 <div class='login-container group'>
   <%= link_to "Sign In", new_session_url, {class: 'sign-up-link'} %>
<form class='login-form' action="<%= users_url %>" method="post">
  <input type="hidden" name="authenticity_token"
         value="<%= form_authenticity_token %>">

    <input
      type="text"
      name="user[email]"
      id="user_email"
      placeholder="Email">
    <br>

    <input
      type="password"
      name="user[password]"
      id="user_password"
      placeholder="Password">
    <br>

    <button>Sign Up</button>
  </form>
   </div>
 </div>
