<!DOCTYPE html>
<html>
<head>
  <title>Sinterest</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <% if signed_in? %>
      <%= button_to 'Log Out', session_url, method: :delete %>
    <% end %>
  </header>


  <% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
      <%= error %>
    <% end %>
  <% end %>


<%= yield %>

</body>
</html>
